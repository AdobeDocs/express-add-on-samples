{
    "name": "licensed-addon",
    "version": "1.0.0",
    "description": "Adobe Creative Cloud Web Add-on.",
    "scripts": {
        "clean": "ccweb-add-on-scripts clean",
        "build": "ccweb-add-on-scripts build --use webpack",
        "start": "ccweb-add-on-scripts start --use webpack",
        "package": "ccweb-add-on-scripts package --use webpack",
        "format": "prettier --write \"src/**/*.js\" \"src/**/*.jsx\" \"src/**/*.json\" \"src/**/*.css\""
    },
    "keywords": [
        "Adobe",
        "Creative Cloud Web",
        "Add-on",
        "panel"
    ],
    "dependencies": {
        "@adobe/react-spectrum": "^3.29.0",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@adobe-ccwebext/ccweb-add-on-scripts": "^1.4.0",
        "@babel/core": "7.18.13",
        "@babel/preset-env": "7.18.10",
        "@babel/preset-react": "7.18.6",
        "babel-loader": "8.2.5",
        "babel-preset-react-app": "10.0.1",
        "copy-webpack-plugin": "11.0.0",
        "css-loader": "6.7.1",
        "html-webpack-plugin": "5.5.0",
        "prettier": "^3.0.2",
        "style-loader": "3.3.1",
        "webpack": "5.77.0",
        "webpack-cli": "5.0.1"
    }
}
